{
    "name": "@wso2apim/publisher",
    "version": "4.0.1",
    "description": "WSO2 API Manager Publisher Web App Note: `eslint-plugin-react` have a fixed version due to implementing prop-types does mark nested destructed props from `eslint-plugin-react` next version onwards\n --max_old_space_size: This is max size of the old space( managed heap) in Mbytes make sure not to increase it more than the memory size of builder machines(Prod build)",
    "keywords": [
        "wso2",
        "developer portal",
        "api marketplace",
        "webpack 5",
        "webpack boilerplate"
    ],
    "main": "/source/index.jsx",
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=16.0.0"
    },
    "scripts": {
        "test:ci": "CI=true jest --silent --ci --watchAll=false --watch=false --coverage --coverageReporters=cobertura",
        "test": "DEBUG_PRINT_LIMIT=9000 jest --watch",
        "test:coverage": "jest --silent --coverage .; python3 -m http.server -d coverage/",
        "build:prod": "NODE_ENVS=production; npm run i18n; npm run i18n:compile; rimraf site/public/dist/ && NODE_OPTIONS=--max_old_space_size=4096 webpack --mode production --stats=errors-only",
        "build:dev": "rimraf site/public/dist/ && webpack --mode development --watch ",
        "analysis": "NODE_ENVS=analysis NODE_OPTIONS=--max_old_space_size=8172 webpack --mode production --progress",
        "lint": "eslint --ignore-pattern '*.test.js' --ignore-pattern '*.test.jsx' --quiet -c .eslintrc.js --ext .jsx,.js source",
        "i18n": "formatjs extract 'source/src/app/**/*.{jsx,tsx}' --throws --out-file site/public/locales/raw.en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "i18n:compile": "formatjs compile site/public/locales/raw.en.json --ast --out-file site/public/locales/en.json",
        "start": "rimraf site/public/dist/ && webpack serve --compress --https --port 8081 --mode development && cp site/public/pages/index.html publisher/",
        "start:mocked": "concurrently npm:mocked 'WSO2_UI_MOCKED=true webpack serve --compress --https --port 8081 --mode production --progress'",
        "mocked": "multi-mock-oas"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/wso2/carbon-apimgt"
    },
    "author": "WSO2 Org",
    "license": "Apache-2.0",
    "dependencies": {
        "@mui/lab": "^5.0.0-alpha.109",
        "@mui/material": "^5.10.15",
        "@mui/styled-engine-sc": "^5.10.14",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "styled-components": "^5.3.6"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
        "@babel/plugin-transform-arrow-functions": "^7.18.6",
        "@babel/plugin-transform-async-to-generator": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/plugin-transform-typescript": "^7.20.0",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@types/jsrsasign": "^10.5.4",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.7.1",
        "eslint": "^8.27.0",
        "eslint-plugin-react": "^7.31.10",
        "file-loader": "^6.2.0",
        "hook-shell-script-webpack-plugin": "^0.1.4",
        "html-webpack-plugin": "^5.5.0",
        "react-intl": "^6.2.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.1",
        "typescript": "^4.8.4",
        "underscore-template-loader": "^1.2.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0"
    }
}
